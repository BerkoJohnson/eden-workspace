{
  "version": 2,
  "name": "pw",
  "builds": [
    { "src": "/dist/apps/pw/*", "use": "@now/static" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/assets/(.*)", "dest": "/dist/apps/pw/assets/$1"},
    { "src": "/(.*).(js|css|ico)", "dest": "/dist/apps/pw/$1.$2" },
    { "src": "/(.*)", "dest": "/dist/apps/pw/index.html" }
  ]
}
